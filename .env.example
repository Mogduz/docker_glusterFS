# .env.example â€” copy to .env and adjust values
# All variables are loaded from this file.
#
# HOST_BRICK_PATHS: comma-separated host paths for Gluster bricks.
# After running `python3 scripts/gen-compose-override.py`, BRICK_PATHS will be set
# to the container target paths like /bricks/brick1,/bricks/brick2,...
ADDRESS_FAMILY=inet
ALLOW_EMPTY_STATE=
ALLOW_FORCE_CREATE=1
AUTH_ALLOW=10.0.0.0/8,192.168.0.0/16,172.16.0.0/12
BRICK_PATH=
BRICK_PATHS=/bricks/brick1,/bricks/brick2
CONTAINER_NAME=gluster-solo
CREATE_VOLUME=1
DATA_PORT_END=49251
DATA_PORT_START=49152
HC_INTERVAL=10s
HC_RETRIES=20
HC_TIMEOUT=5s
HOSTNAME=
HOSTNAME_GLUSTER=gluster-solo
HOST_BRICK_PATHS=/mnt/disk1/gluster/brick1,/mnt/disk2/gluster/brick2
LOG_LEVEL=INFO
MAX_PORT=60999
MGMT_PORT1=24007
MGMT_PORT2=24008
MODE=
NFS_DISABLE=1
PEERS=
REPLICA=2
REQUIRE_ALL_PEERS=
TRANSPORT=
TZ=UTC
UMASK=
VOLNAME=gv0
VOL_OPTS=performance.client-io-threads=on,cluster.quorum-type=auto
VTYPE=replica
bp=
force=
host=
